{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "jest": true
  },
  "plugins": [
    "jest",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off"
  }
}